<?xml version="1.0"?>
<package>
  <metadata>
    <id>Loyc.Utilities</id>
    <version>$version$</version>
    <authors>David Piepgrass</authors>
    <owners>David Piepgrass</owners>
    <projectUrl>http://core.loyc.net</projectUrl>
    <!--<iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>-->
